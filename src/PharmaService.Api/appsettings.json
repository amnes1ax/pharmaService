{
  "ConnectionStrings": {
    "main": "Host=localhost; port=5433; database=pharma_service; username=app; password=password;"
  },
  "Authentication": {
  },
  "Integrations": {
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.AspNetCore",
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Context",
      "Serilog.Enrichers.Thread",
      "Serilog.Enrichers.Span",
      "Serilog.Enrichers.Sensitive",
      "Serilog.Expressions",
      "Serilog.Exceptions",
      "Serilog.Sinks.Async",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq",
      "Serilog.Formatting.Compact"
    ],
    "Filter": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
        "MassTransit": "Warning"
      }
    },
    "WriteTo": [
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithProperty",
      "WithSensitiveDataMasking",
      "WithSpan",
      "WithExceptionDetails"
    ],
    "Properties": {
      "Application": "PharmaService.Api"
    }
  },
  "OpenTelemetry": {
    "ApplicationName": "PharmaService.Api",
    "Enable": false
  },
  "Cors": {
    "Name": "AnyOrigins"
  }
}
